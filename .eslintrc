{
    "plugins": [
      "prettier"
    ],
    "rules": {
      "no-console": 0,
      "no-underscore-dangle": 0,
      "no-param-reassign": 0,
      "react-hooks/rules-of-hooks": 0,
      "no-nested-ternary": 0,
      "no-else-return": 0,
      "import/prefer-default-export": 0,
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "next"
        }
      ],
      "no-use-before-define": [
        "warn",
        {
          "variables": false
        }
      ],
      "no-multi-str": 0,
      "react/prop-types": 0,
      "import/no-extraneous-dependencies": [
        "warn"
      ],
      "import/imports-first": [
        "error",
        "absolute-first"
      ],
      "import/newline-after-import": "error",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    },
    "env": {
      "node": true,
      "mocha": true,
      "jest": true,
    },
    "parserOptions": {
      "ecmaVersion": 8
    },
    "extends": [
      "airbnb-base",
      "plugin:prettier/recommended",
      "prettier/react",
      "react-app"
    ]
  }